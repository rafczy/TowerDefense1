<Window x:Class="TowerDefense1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TowerDefense1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="MySpace" AutomationProperties.Name="MySpace">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="reset" Content="reset" HorizontalAlignment="Left" Margin="10,166,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="reset_Click"/>
        <Button x:Name="nextTurn" Content="next turn" HorizontalAlignment="Left" Margin="106,166,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="nextTurn_Click"/>






    </Grid>
</Window>
